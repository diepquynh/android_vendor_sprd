<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true"  android:state_enabled="true"
          android:drawable="@drawable/edit_undo_pressed" /> <!-- pressed -->
    <item android:state_focused="true"  android:state_enabled="true"
          android:drawable="@drawable/edit_undo_pressed" /> <!-- focused -->

    <item android:state_pressed="false"  android:state_enabled="true"
          android:drawable="@drawable/edit_undo_focus" />

    <item android:state_focused="false"  android:state_enabled="true"
          android:drawable="@drawable/edit_undo_focus" />

    <item android:drawable="@drawable/edit_undo_not_focus" /> <!-- default -->
</selector>
